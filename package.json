{
	"author": "Renildo Pereira",
	"description": "Yumma CSS Intellisense for VS Code.",
	"displayName": "Yumma CSS Intellisense",
	"homepage": "https://yummacss.com",
	"icon": "icon.png",
	"license": "MIT",
	"main": "./out/extension.js",
	"name": "yumma-css-intellisense",
	"private": true,
	"publisher": "yumma-css",
	"repository": {
		"type": "git",
		"url": "https://github.com/yumma-lib/yumma-css-intellisense"
	},
	"version": "0.1.0",
	"activationEvents": [
		"onLanguage:django-html",
		"onLanguage:ejs",
		"onLanguage:html",
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:jsx",
		"onLanguage:php",
		"onLanguage:pug",
		"onLanguage:typescript",
		"onLanguage:vue-html",
		"onLanguage:vue"
	],
	"dependencies": {
		"braces": "^3.0.3",
		"tinycolor2": "^1.6.0"
	},
	"devDependencies": {
		"@types/node": "^18",
		"@types/tinycolor2": "^1.4.6",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.4.2"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"keywords": [
		"css",
		"intellisense",
		"vscode",
		"vscode-extension",
		"yummacs"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	}
}